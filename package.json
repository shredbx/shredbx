{
  "name": "shredbx",
  "version": "1.0.0",
  "private": false,
  "scripts": {
    "dev": "pnpm build:shared && concurrently \"pnpm dev:shared\" \"pnpm dev:reactbook-web\" \"pnpm dev:web\"",
    "dev:shared": "pnpm --filter @shredbx/shared dev",
    "dev:web": "pnpm --filter @shredbx/web dev",
    "dev:reactbook-web": "pnpm --filter @shredbx/reactbook-web dev",
    "build:shared": "pnpm --filter @shredbx/shared build",
    "dev:reactbook": "pnpm --filter @shredbx/reactbook dev",
    "build": "pnpm build:packages && pnpm build:apps",
    "build:packages": "pnpm --filter './src/packages/*' build",
    "build:apps": "pnpm --filter './src/apps/*' build",
    "clean": "pnpm -r clean",
    "install:all": "pnpm install",
    "type-check": "pnpm -r type-check",
    "test": "pnpm --filter @shredbx/reactbook test",
    "test:watch": "pnpm --filter @shredbx/reactbook test:watch",
    "test:coverage": "pnpm --filter @shredbx/reactbook test:coverage"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "concurrently": "^9.2.0",
    "typescript": "^5.0.0"
  },
  "packageManager": "yarn@4.9.1+sha512.f95ce356460e05be48d66401c1ae64ef84d163dd689964962c6888a9810865e39097a5e9de748876c2e0bf89b232d583c33982773e9903ae7a76257270986538"
}
