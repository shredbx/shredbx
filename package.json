{
  "name": "reactbook",
  "version": "1.0.0",
  "private": false,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "dev:all": "turbo dev --parallel",
    "dev:web": "turbo dev --filter=@web",
    "dev:playground": "turbo dev --filter=@reactbook/playground",
    "test": "turbo test",
    "test:watch": "turbo test --watch",
    "test:coverage": "turbo test --coverage",
    "type-check": "turbo type-check"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "concurrently": "^9.2.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "ts-jest": "^29.4.1",
    "turbo": "^2.5.6",
    "typescript": "^5.0.0"
  },
  "packageManager": "pnpm@9.0.0"
}
