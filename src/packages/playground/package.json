{
  "name": "@playground",
  "version": "0.0.1",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./mcp/server": {
      "types": "./src/mcp/server/index.ts",
      "import": "./src/mcp/server/index.ts"
    },
    "./mcp/*": {
      "types": "./src/mcp/*/index.ts",
      "import": "./src/mcp/*/index.ts"
    },
    "./mcp-client": {
      "types": "./src/mcp-client/index.ts",
      "import": "./src/mcp-client/index.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@ui-web": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "react": "^19.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}
