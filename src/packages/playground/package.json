{
  "name": "@reactbook/playground",
  "version": "1.0.0",
  "private": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./mcp/server": {
      "import": "./dist/mcp/server/index.js",
      "require": "./dist/mcp/server/index.cjs",
      "types": "./dist/mcp/server/index.d.ts"
    },
    "./patternbook": {
      "import": "./dist/patternbook/index.js",
      "require": "./dist/patternbook/index.cjs",
      "types": "./dist/patternbook/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@reactbook/shared-deps": "workspace:*",
    "@reactbook/shared-ui-deps": "workspace:*",
    "@reactbook/ui-web": "workspace:*"
  },
  "devDependencies": {
    "@reactbook/typescript-config": "workspace:*"
  },
  "peerDependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  }
}
