{
  "name": "@reactbook/playground",
  "version": "0.0.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./mcp/server": {
      "import": "./dist/mcp/server/index.js",
      "require": "./dist/mcp/server/index.cjs",
      "types": "./dist/mcp/server/index.d.ts"
    },
    "./patternbook": {
      "import": "./dist/patternbook/index.js",
      "require": "./dist/patternbook/index.cjs",
      "types": "./dist/patternbook/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@reactbook/ui-web": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "react": "^19.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}
