    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.511.0",
    "next-themes": "^0.4.6",
    "tailwind-merge": "^3.3.0"
  },
  "devDependencies": {
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@tailwindcss/postcss": "^4.1.11",
    "autoprefixer": "^10.4.20",
    "postcss": "^8",
    "tailwindcss": "^4.1.11",
    "tsup": "^8.0.0",
    "typescript": "^5"
  }
